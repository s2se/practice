   "source": [
    "import mglearn\n",
    "X, y = mglearn.datasets.make_forge()\n",
    "# 산점도\n",
    "mglearn.discrete_scatter(X[:, 0], X[:, 1], y)\n",
    "plt.legend([\"클래스 0\", \"클래스 1\"], loc=4)\n",
    "plt.xlabel(\"첫 번째 특성\")\n",
    "plt.ylabel(\"두 번째 특성\")\n",
    "# 데이터포인트 26개와 특성 2개\n",
    "print(\"X.shape: {}\".format(X.shape))"
